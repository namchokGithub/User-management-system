@model UserManagementSystem.Models.Account

<div>
    <h4>Account</h4>
    <hr />
    <dl class="row">

        @foreach (var item in ViewData["Account"] as List<Account>)
        {
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.acc_User)
            </dt>   
            <dd class="col-sm-10">
                @Html.DisplayFor(modelItem => item.acc_User)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.acc_IsActive)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(modelItem => item.acc_IsActive)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.acc_password)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(modelItem => item.acc_password)
            </dd>
        }

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>

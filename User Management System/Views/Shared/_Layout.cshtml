@*
    Namespace: ~/View/Shared
    File: _Layout.cshtml
    Description: Layout template.
*@

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - User Management System</title>
    <link rel="icon" href="~/img/logoUMS.png" type="image/icon type">

</head>
<body>
    <!-- content -->
    <div class="content">
        <div class="container-fluid">
            @RenderBody() <!-- Rander the content -->
        </div>
    </div>
    <!-- ./content -->

    @RenderSection("Scripts", required: false)
</body>
</html>
<!-- End html -->